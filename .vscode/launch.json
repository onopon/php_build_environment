{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Main",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/sapi/cli/php",
      "args": [
        "${workspaceFolder}/test.php"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "logging": {
        "trace": true,
        "engineLogging": true,
        "traceResponse": true
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/src/php-src/Zend",
      "program": "/src/php-src/Zend/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}